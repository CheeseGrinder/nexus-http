{
  "name": "nexus-http",
  "version": "1.3.1",
  "description": "nexus-http is a simple http client that allows you to make requests easily.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "tsc -b tsconfig.build.json",
    "start": "tsc -w",
    "format": "prettier --write \"{src,test}/**/*.ts\"",
    "check": "prettier --check \"{src,test}/**/*.ts\"",
    "lint": "eslint \"{src,test}/**/*.ts\" --fix",
    "test": "jest --runInBand",
    "test:watch": "jest --watch",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CheeseGrinder/simple-http.git"
  },
  "keywords": [
    "typescript",
    "http",
    "client"
  ],
  "author": {
    "name": "CheeseGrinder",
    "email": "devs.cheese@gmail.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/CheeseGrinder/nexus-http/issues"
  },
  "homepage": "https://github.com/CheeseGrinder/nexus-http#readme",
  "dependencies": {},
  "devDependencies": {
    "@types/jest": "^27.0.2",
    "@typescript-eslint/eslint-plugin": "^5.2.0",
    "@typescript-eslint/parser": "^5.2.0",
    "eslint": "^8.1.0",
    "jest": "^27.3.1",
    "prettier": "^2.4.1",
    "ts-jest": "^27.0.7",
    "ts-node": "^10.4.0",
    "typescript": "^4.4.4"
  }
}
